#some test code:


packageFolder = "~/Rncs/simpleCache";
install.packages(packageFolder, repos=NULL)

library(simpleCache)
setLocalCacheDir("~")

normSample = rnorm(1e7, 0,1)

simpleCache("normSample", "rnorm(1e7, 0,1)", recreate=TRUE)

simpleCache("normSample", "rnorm(1e7, 0,1)")
refreshPackage("simpleCache")




